{
  "name": "totem-message-server",
  "version": "1.0.0",
  "description": "Totem Internal Message Server",
  "main": "index.js",
  "scripts": {
    "server": "nodemon --watch src/ --ignore src/data --verbose --exec ./node_modules/.bin/babel-node ./src/index.js --ext js --presets env",
    "dev": "yarn run server",
    "prod": "nodemon --verbose --exec ./node_modules/.bin/babel-node ./src/index.js --ext js --presets env",
    "tools-company": "./node_modules/.bin/babel-node ./tools/companiesCSVToJSON.js --ext js --presets env"
  },
  "babel": {
    "plugins": [
      "babel-plugin-transform-object-rest-spread"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "keywords": [
    "totem"
  ],
  "contributors": [
    {
      "name": "Chris DCosta",
      "email": "chris.dcosta@totemaccounting.com"
    },
    {
      "name": "Toufiqur R. Chowdhury",
      "email": "htr.letun@gmail.com"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@polkadot/api": "^0.100.1",
    "@polkadot/keyring": "^1.8.1",
    "@polkadot/util": "^1.8.1",
    "@polkadot/wasm-crypto": "^0.14.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bip39": "^3.0.2",
    "blakejs": "^1.1.0",
    "bs58": "^4.0.1",
    "create-hash": "^1.2.0",
    "csvtojson": "^2.0.10",
    "mongodb": "^3.5.4",
    "nano": "^8.2.0",
    "node-localstorage": "^2.1.5",
    "oo7": "^0.7.12",
    "oo7-substrate": "^0.8.0",
    "pbkdf2": "^3.0.17",
    "socket.io": "^2.3.0",
    "tweetnacl-util": "^0.15.0",
    "uuid": "^3.3.3",
    "uws": "^10.148.1",
    "xxhashjs": "^0.2.2"
  }
}
